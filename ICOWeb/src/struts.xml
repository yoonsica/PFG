<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name="struts.objectFactory" value="spring" />
<constant name="struts.devMode" value="true"></constant>
	<package name="struts2" namespace="/" extends="struts-default">
		<action name="icoConfig" class="icoConfigAction">
			<result>/index.jsp</result>
		</action>
		<action name="conditionAction" class="conditionAdd">
			<result>/addNewCondition.jsp</result>
		</action>
	</package>
	<package name="ajax" namespace="/" extends="json-default">
		<action name="conditionAdd" class="conditionAdd" method="add">
			<result type="json" name="json"></result>
		</action>
	</package>
</struts>    
