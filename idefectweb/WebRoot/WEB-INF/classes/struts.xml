<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.devMode" value="true"></constant>
	<package name="struts2" namespace="/" extends="struts-default">
		<action name="unitDevice" class="unitDeviceAction">
			<result>/module/unitDeviceStatistic.jsp</result>
		</action>
		<action name="voltageClass" class="voltageClassAction">
			<result>/module/voltageClassStatistic/voltageClassStatics.jsp</result>
		</action>
	</package>
	
	<package name="ajax" extends="json-default">
		<action name="voltageClassAjax" class="voltageClassAction" method="ajax">
			<result type="json" name="json"></result>
		</action>
		<action name="config" class="unitDeviceAction" method="config">
			<result type="json" name="json"></result>
		</action>
	</package>
</struts>    
